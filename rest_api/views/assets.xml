<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <template id="assets_backend" name="rest_api assets" inherit_id="web.assets_backend">
        
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/css" href="/rest_api/static/lib/jquery.json-viewer/jquery.json-viewer.css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/rest_api/static/lib/jquery.json-viewer/jquery.json-viewer.js"></script>
        </xpath>
        
    </template>
    
</odoo>
